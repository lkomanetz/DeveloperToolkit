<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageId>Dtk.Extensions</PackageId>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Logan Komanetz</Authors>
    <Product>Developer's Toolkit</Product>
    <Description>Type extensions for the .NET Developer toolkit</Description>
    <PackageProjectUrl>https://github.com/lkomanetz/DeveloperToolkit/tree/dev/src/dotnet</PackageProjectUrl>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <RepositoryType>Git</RepositoryType>
    <PackageReleaseNotes>Initial Release</PackageReleaseNotes>
  </PropertyGroup>

</Project>
